<div class="row">
  <div class="col s12">
    <h3 class="center-align">Lista de Estudantes</h3>
  </div>
</div>
<div class="row">
  <% @students.each do |student| %>
    <div class="col s6 m6  hoverable">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Nome: <%= student.name %></span>
          <p >Email: <%= student.email %></p>
          <p>lorem ipsum dolor vade mecum</p>
        </div>
        <div class="card-action">
          <%= link_to "Ver Perfil", student %>
          <a href="#">Info</a>
        </div>
      </div>
    </div>
  <% end %>
</div>
