<div class="row">
  <div class="col s6 m6 card-panel hoverable">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Nome: <%= @student.name %></span>
        <p >Email: <%= @student.email %></p>
        <p>lorem ipsum dolor vade mecum</p>
      </div>
      <div class="card-action">
        <%= link_to edit_student_path(@student) do %>
          <i class="tiny material-icons">create</i>
          Editar Perfil
        <% end %>
        <a href="#">Info</a>
      </div>
    </div>
  </div>
</div>
